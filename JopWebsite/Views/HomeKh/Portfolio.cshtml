@model IEnumerable<JopWebsite.Models.Profile>

@{
    ViewBag.Title = "Portfolio";
    Layout = "~/Views/Shared/_Layoutkh.cshtml";
}

<!--/ Section Portfolio Star-->
<section id="work" class="portfolio-mf sect-pt4 route">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="title-box text-center">
                    <h3 class="title-a">
                        اللبوم
                    </h3>
                    <p class="subtitle-a">
                        بعض من الصور
                    </p>
                    <div class="line-mf"></div>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {


                <div class="col-md-4">
                    <div class="work-box">
                        <a href="~/Uploads/@item.ProImage" data-lightbox="gallery-mf">
                            <div class="work-img">
                                <img src="~/Uploads/@item.ProImage" alt="" class="img-fluid">
                            </div>
                            <div class="work-content">
                                <div class="row">
                                    <div class="col-sm-8">
                                        <div class="w-more">
                                            <span class="w-ctegory"> تصميم</span> / <span class="w-date">@item.ProDate</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="w-like">
                                            <span class="ion-ios-plus-outline"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
</section>


