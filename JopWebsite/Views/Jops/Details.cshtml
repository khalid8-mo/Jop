@model JopWebsite.Models.Jop

@{
    ViewBag.Title = "تفاصيل الوظائف";
        Layout = "~/Views/Shared/_Layoutkh.cshtml";

}
<div class="col-md-4">
    <div class="card" style="margin-left:20px;margin-bottom:30px;height:380px">
        <img src="~/Uploads/@Html.DisplayFor(model => model.JopImage)" style="width:100%;height: 197px;" />
        <div class="card-block">
            <div class="card-moved">
                <h3>اسم الوظيفه : @Html.DisplayFor(model => model.JopTitle)</h3>
                <h5> وصف الوظيفه : @Html.DisplayFor(model => model.JopContent)</h5>
            </div>
            <button class="btn btn-primary">التقدم الي الوظيفه</button>
            <a href="@Url.Action("Index","Home")" class="btn btn-outline-brown"><span class="glyphicon glyphicon-backward"></span> عوده</a>

        </div>
    </div>
</div>
<div class="col-md-8"></div>


  
